{
    "name": "@mat3ra/mode",
    "version": "0.0.0",
    "description": "MOdel and method DEfinitions",
    "scripts": {
        "test": "nyc --reporter=text mocha --extension ts --recursive --bail --require ts-node/register/transpile-only \"tests/**/*.test.ts\"",
        "pretest": "npm run transpile",
        "lint": "eslint src tests && prettier --write src tests",
        "lint:fix": "eslint --fix --cache src tests && prettier --write src tests",
        "transpile": "npm run transpile:js && npm run transpile:ts",
        "transpile:js": "babel --out-dir dist src",
        "transpile:ts": "tsc --project tsconfig.json",
        "postinstall": "npm run transpile",
        "prettier": "prettier --check src tests",
        "prepare": "husky install || exit 0",
        "prepublishOnly": "rm -rf dist; npm run transpile"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Exabyte-io/mode.git"
    },
    "main": "dist/index.js",
    "types": "dist/js/index.d.ts",
    "files": [
        "/dist",
        "/src",
        ".babelrc"
    ],
    "author": "Exabyte Inc.",
    "bugs": {
        "url": "https://github.com/Exabyte-io/mode/issues"
    },
    "license": "Apache-2.0",
    "homepage": "https://github.com/Exabyte-io/mode",
    "dependencies": {
        "@babel/cli": "7.16.0",
        "@babel/core": "7.16.0",
        "@babel/eslint-parser": "7.16.3",
        "@babel/plugin-proposal-class-properties": "7.16.0",
        "@babel/preset-env": "7.16.4",
        "@babel/preset-react": "7.16.7",
        "@babel/register": "^7.16.0",
        "@babel/runtime-corejs3": "7.16.8",
        "ajv": "^8.12.0",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "mixwith": "^0.1.1",
        "underscore": "^1.13.3",
        "underscore.string": "^3.3.4"
    },
    "devDependencies": {
        "@exabyte-io/eslint-config": "^2022.11.17-0",
        "@mat3ra/esse": "2024.4.19-1",
        "@mat3ra/standata": "2025.10.2-0",
        "@mat3ra/code": "2024.3.25-3",
        "@types/chai": "^4.3.11",
        "@types/lodash": "^4.14.202",
        "@types/mocha": "^10.0.6",
        "@types/underscore": "^1.11.14",
        "chai": "^4.3.4",
        "eslint": "7.32.0",
        "eslint-config-airbnb": "19.0.2",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-exports": "^1.0.0-beta.2",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.1.0",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "7.30.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "mocha": "^9.1.3",
        "nyc": "^15.1.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.0"
    },
    "peerDependencies": {
        "@mat3ra/esse": "*",
        "@mat3ra/standata": "*",
        "@mat3ra/code": "*"
    },
    "engines": {
        "node": ">20.0.0"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css}": "prettier --write"
    }
}
